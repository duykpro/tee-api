{
  "name": "tee-api-server",
  "version": "0.0.1",
  "description": "Tee API Server",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run tslint && npm run build-ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.0.0",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "lodash": "^4.17.10",
    "mysql2": "^1.5.3",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "semver": "^5.5.0",
    "sequelize": "^4.38.0",
    "sharp": "^0.20.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/camelcase": "^4.1.0",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.112",
    "@types/mysql2": "github:types/mysql2",
    "@types/ramda": "^0.25.35",
    "@types/semver": "^5.5.0",
    "@types/sequelize": "^4.27.24",
    "@types/sharp": "^0.17.9",
    "@types/uuid": "^3.4.3",
    "concurrently": "^3.6.0",
    "nodemon": "^1.18.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
